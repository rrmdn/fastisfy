{
  "name": "fastisfy",
  "version": "0.1.0",
  "description": "A fast and simple framework for building REST APIs with Fastify using filesystem routing",
  "author": "Rizki Romadhoni <rrmdn@pm.me>",
  "homepage": "https://github.com/rrmdn/fastisfy",
  "license": "MIT",
  "bin": {
    "fastisfy": "./bin/run"
  },
  "types": "src/types/fastisfy.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "fastisfy",
    "dirname": "fastisfy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "fastisfy-dev": "./bin/run",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "ts-mocha -p tsconfig.json --exit \"src/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/rrmdn/fastisfy/issues",
  "keywords": [
    "oclif"
  ],
  "devDependencies": {
    "@oclif/test": "^2.2.16",
    "@swc/core": "^1.3.24",
    "@types/chai": "^4",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.10",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/compress": "^6.2.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/swagger": "^8.2.1",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/under-pressure": "^8.2.0",
    "@oclif/core": "^1.22.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.9",
    "@sinclair/typebox": "^0.25.16",
    "chokidar": "^3.5.3",
    "fastify": "^4.10.2",
    "fastify-oas": "^3.0.8",
    "module-from-string": "^3.3.0"
  }
}
